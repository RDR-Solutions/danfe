{
  "name": "danfe-parser",
  "version": "1.0.9",
  "description": "Parse DANFE XML (NFC-e, NFe) into a normalized Danfe object",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "danfe",
    "nfe",
    "nfce",
    "xml",
    "brazil",
    "nota-fiscal",
    "fiscal"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "fast-xml-parser": "^4.3.2"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
